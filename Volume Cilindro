programa {
    inclua biblioteca Matematica --> mat 

/*  Fa√ßa um algoritmo para calcular o volume de um cilindro(pi*r^2*h)  */

  funcao inicio() {
    
    inteiro r,h;
    real pi = 3.14
    real aux;

    escreva(" h ")
		leia(h)
    escreva(" r ")
		leia(r)

    aux = (pi*(mat.potencia(r,2)*h)

    escreva(aux)
  }
}

