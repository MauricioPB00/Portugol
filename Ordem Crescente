programa {
    inclua biblioteca Matematica --> mat 

/* 
Ler 2 valores e escrevÃª-los em ordem crescente
*/

  funcao inicio() {
    
   real num, num2;

   escreva(" numero 01 " )
   leia(num)

   escreva(" numero 02 " )
   leia(num2)

  se(num > num2){
    escreva(num2 , num)
  }
  senao{
    escreva(num , num2)
  }
}
